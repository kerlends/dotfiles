URxvt*font:				xft:Ubuntu Mono derivative Powerline:size=12:antialias=true,xft:Liberation Mono:antialias=true:hinting=true,Awesome 10
URxvt*boldFont:				xft:Ubuntu Mono derivative Powerline:size=12:antialias=true,xft:Liberation Mono:antialias=true:hinting=true
URxvt*scrollBar:		false
URxvt*matcher.button:	1
!URxvt.letterSpace:		-1

URxvt.background:		#2f343f

URxvt*cursorBlink:      	true
URxvt*cursorColor:      	#FFFFFF

Xft.dpi:				96
Xft.antialias:			true
Xft.autohint:			false
Xft.hinting:			true
Xft.hintstyle:			hintslight
Xft.rgba:				rgb
Xft.lcdfilter:			lcddefault
ntilias:				true

URxvt.perl-ext-common:	default,clipboard
URxvt.iso14755:			false

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy

URxvt.clipboard.copycmd:	xsel -ib
Urxvt.clipboard.pastecmd:	xsel -ob

